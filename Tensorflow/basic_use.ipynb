   "outputs": [],
   "source": [
    "# 创建一个常量 \n",
    "ml = tf.constant([[3, 3]])\n",
    "m2 = tf.constant([[2], [3]])\n",
    "#  矩阵相乘 \n",
    "product = tf.matmul(ml, m2)\n",
    "print(product)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
