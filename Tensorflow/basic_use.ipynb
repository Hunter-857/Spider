      "[[15]]\n"
     ]
    }
   ],
   "source": [
    "# 定义一个会话 来执行 orperate\n",
    "\n",
    "with tf.Session() as session:\n",
    "    result = session.run(product)\n",
    "    print(result)\n"
   ]
